<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.updatingtech.videosaas.model.Video" table="video">
        <id name="videoId" type="java.lang.Long">
            <column name="video_id" />
            <generator class="identity" />
        </id>
        <property name="videoName" type="string">
            <column name="video_name" length="45" not-null="true" />
        </property>
        <property name="videoSize" type="string">
            <column name="video_size" not-null="true" />
        </property>
        <property name="category" type="string">
            <column name="category" not-null="true" />
        </property>
        <property name="level" type="java.lang.integer">
            <column name="level" not-null="true" />
        </property>
        <property name="viewPermission" type="java.lang.Integer">
            <column name="view_permission" not-null="true" />
        </property>
        <property name="uploadDate" type="timestamp">
            <column name="upload_date" length="19" not-null="true" />
        </property>
    </class>
</hibernate-mapping>