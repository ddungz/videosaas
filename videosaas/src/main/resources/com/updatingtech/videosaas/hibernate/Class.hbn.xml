<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.updatingtech.videosaas.model.Class" table="class">
        <id name="classId" type="java.lang.Long">
            <column name="class_id" />
            <generator class="identity" />
        </id>
        <property name="className" type="string">
            <column name="class_name" length="45" not-null="true" />
        </property>
        <property name="classLevel" type="string">
            <column name="class_level" not-null="true" />
        </property>
        <property name="category" type="string">
            <column name="category" not-null="true" />
        </property>
    </class>
</hibernate-mapping>